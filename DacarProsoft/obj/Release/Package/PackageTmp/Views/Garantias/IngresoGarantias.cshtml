                                        <label style="padding:0px 10px 0px 0px" class="form-check-label" for="inlineRadio1">Si</label>
                                        <input class="form-check-input" type="radio" name="inlineRadioOptions10" id="inlineRadio1" value="false">
                                        <label class="form-check-label" for="inlineRadio1">No</label>
                                    </div>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-center">
                                    Hinchada
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="inlineRadioOptions11" id="inlineRadio1" value="true">
                                        <label style="padding:0px 10px 0px 0px" class="form-check-label" for="inlineRadio1">Si</label>
                                        <input class="form-check-input" type="radio" name="inlineRadioOptions11" id="inlineRadio1" value="false">
                                        <label class="form-check-label" for="inlineRadio1">No</label>
                                    </div>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-center">
                                    Bornes Flojos o Hundidos
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="inlineRadioOptions12" id="inlineRadio1" value="true">
                                        <label style="padding:0px 10px 0px 0px" class="form-check-label" for="inlineRadio1">Si</label>
                                        <input class="form-check-input" type="radio" name="inlineRadioOptions12" id="inlineRadio1" value="false">
                                        <label class="form-check-label" for="inlineRadio1">No</label>
                                    </div>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-center">
                                    Bornes Fundidos
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="inlineRadioOptions13" id="inlineRadio1" value="true">
                                        <label style="padding:0px 10px 0px 0px" class="form-check-label" for="inlineRadio1">Si</label>
                                        <input class="form-check-input" type="radio" name="inlineRadioOptions13" id="inlineRadio1" value="false">
                                        <label class="form-check-label" for="inlineRadio1">No</label>
                                    </div>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-center">
